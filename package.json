{
  "name": "phonebook-api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run build && node ./build/app.js",
    "start:watch": "npm run build:watch & nodemon ./build/app.js",
    "build": "rm -rf ./build && tsc",
    "build:watch": "npm run build -- -w",
    "start:docker": "docker-compose run --rm -v $(pwd):/app api run start",
    "watch:docker": "docker-compose run --rm -v $(pwd):/app api run start:watch",
    "test:docker": "docker-compose run -u root --rm -v $(pwd):/app api run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c-kle/phonebook-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/c-kle/phonebook-api/issues"
  },
  "homepage": "https://github.com/c-kle/phonebook-api#readme",
  "dependencies": {
    "@types/node": "^10.12.7",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "jsonwebtoken": "8.4.0",
    "multer": "^1.4.1",
    "mysql2": "^1.6.4",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "ramda": "^0.25.0",
    "reflect-metadata": "^0.1.12",
    "routing-controllers": "^0.7.7",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.9",
    "typeorm-typedi-extensions": "^0.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "8.3.0",
    "@types/multer": "^1.3.7",
    "@types/ramda": "github:types/npm-ramda#dist",
    "nodemon": "^1.18.6",
    "typescript": "^3.1.6"
  }
}
